GATE ZERO    1 Y=CONST0;
GATE ONE     1 Y=CONST1;
GATE $_BUF_    1     Y=A;                  PIN * NONINV  1 999 1 0 1 0
GATE $_NOT_    1     Y=!A;                 PIN * INV     1 999 1 0 1 0
GATE $_AND_    4     Y=A*B;                PIN * NONINV  1 999 1 0 1 0
GATE $_OR_     4     Y=A+B;                PIN * NONINV  1 999 1 0 1 0
GATE $_NOR_    4     Y=!(A+B);             PIN * INV     1 999 1 0 1 0
GATE $_XOR_    4     Y=(A*!B)+(!A*B);      PIN * UNKNOWN 1 999 1 0 1 0
